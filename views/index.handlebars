<div>
    <h1 class="card z-depth-3 center-align">Read the latest from 'The Verge'</h1>
    <div class="row">
        
        <h5 class="card z-depth-3 left-align col s6">Click 'Scrape' to get the latest articles</h5>
    </div>
</div>
<div class="row">
    
    <div class="col s10 left-align">
        {{#each article}}
        <div class="card-panel">
            <h4>{{this.title}}</h4>
            <a href="/readArticle/{{this._id}}" class="waves-effect waves-light btn"id="btnSignUp">Read Article</a>
        </div>
        {{/each}}
    </div>
    <div class="col s1"></div>
</div>